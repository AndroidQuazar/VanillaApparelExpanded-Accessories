  <ThingDef ParentName="ApparelBase">
    <defName>VAEA_Apparel_MedicBag</defName>
    <label>medic bag</label>
    <description>A bag with everything needed to provide medical treatment.</description>
    <techLevel>Industrial</techLevel>
    <recipeMaker>
	  <researchPrerequisite>MedicineProduction</researchPrerequisite>
      <recipeUsers>
        <li>DrugLab</li>
      </recipeUsers>
      <useIngredientsForColor>false</useIngredientsForColor>
      <workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
      <workSkill>Intellectual</workSkill>
      <effectWorking>Tailor</effectWorking>
      <soundWorking>Recipe_Tailor</soundWorking>
      <skillRequirements>
        <Crafting>4</Crafting>
        <Intellectual>4</Intellectual>
      </skillRequirements>
    </recipeMaker>
    <costList>
      <MedicineIndustrial>4</MedicineIndustrial>
    </costList>
    <graphicData>
      <texPath>Things/Apparel/MedicBag/MedicBag</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <WorkToMake>2000</WorkToMake>
      <Mass>2</Mass>
      <Flammability>0.4</Flammability>
      <EquipDelay>1</EquipDelay>
    </statBases>
    <thingCategories>
      <li>ApparelUtility</li>
    </thingCategories>
    <generateAllowChance>0.15</generateAllowChance>
	<equippedStatOffsets>
		<MedicalSurgerySuccessChance>0.2</MedicalSurgerySuccessChance>
		<MedicalTendQuality>0.15</MedicalTendQuality>
	</equippedStatOffsets>
    <apparel>
	  <wornGraphicPath>Things/Apparel/MedicBag/MedicBag</wornGraphicPath>
      <careIfWornByCorpse>false</careIfWornByCorpse>
      <careIfDamaged>false</careIfDamaged>
      <bodyPartGroups>
        <li>Waist</li>
      </bodyPartGroups>
      <layers>
        <li>Belt</li>
      </layers>
      <tags>
		<li>VAEA_Utility_Industrial</li>
      </tags>
      <wornGraphicData>
        <renderUtilityAsPack>true</renderUtilityAsPack>
        <north>
          <male>  <offset>(-0.3, -0.25)</offset> </male>
          <female><offset>(-0.3,-0.3)</offset> </female>
          <thin>  <offset>(-0.2,-0.25)</offset> </thin>
          <hulk>  <offset>(-0.33,-0.4)</offset> </hulk>
          <fat>   <offset>(-0.5,-0.25)</offset> </fat>
        </north>
        <south>
          <male>  <offset>(0.3, -0.25)</offset> </male>
          <female><offset>(0.3,-0.3)</offset> </female>
          <thin>  <offset>(0.2,-0.25)</offset> </thin>
          <hulk>  <offset>(0.33,-0.4)</offset> </hulk>
          <fat>   <offset>(0.5,-0.25)</offset> </fat>
        </south>
        <east>
          <offset>(-0.05,-0.3)</offset>
		  <hulk><offset>(-0.1,-0.1)</offset></hulk>
		  <thin><offset>(-0.1,-0.0)</offset></thin>
		  <female><offset>(-0.05,-0.0)</offset> </female>
        </east>
        <male> <scale>(0.6,0.6)</scale></male>
        <female><scale>(0.6,0.6)</scale></female>
        <thin><scale>(0.7,0.7)</scale></thin>
        <hulk><scale>(0.75,0.75)</scale></hulk>
        <fat> <scale>(0.75,0.75)</scale></fat>
