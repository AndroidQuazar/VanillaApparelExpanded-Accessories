<Defs>

  <ThingDef ParentName="ApparelBase">
    <defName>VAEA_Apparel_Backpack</defName>
    <label>backpack</label>
    <description>A simple roomy backpack. Greatly increases the carry weight of the wearer.</description>
    <techLevel>Neolithic</techLevel>
    <recipeMaker>
      <unfinishedThingDef>UnfinishedApparel</unfinishedThingDef>
      <recipeUsers>
        <li>CraftingSpot</li>
		<li>HandTailoringBench</li>
		<li>ElectricTailoringBench</li>
      </recipeUsers>
      <useIngredientsForColor>false</useIngredientsForColor>
      <workSpeedStat>GeneralLaborSpeed</workSpeedStat>
      <workSkill>Crafting</workSkill>
      <effectWorking>Tailor</effectWorking>
      <soundWorking>Recipe_Tailor</soundWorking>
      <skillRequirements>
        <Crafting>3</Crafting>
      </skillRequirements>
    </recipeMaker>
    <stuffCategories>
      <li>Fabric</li>
      <li>Leathery</li>
    </stuffCategories>
	<costStuffCount>50</costStuffCount>
    <graphicData>
      <texPath>Things/Apparel/Backpack/Backpack</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <statBases>
      <WorkToMake>2400</WorkToMake>
      <Mass>2</Mass>
      <Flammability>0.4</Flammability>
      <EquipDelay>2</EquipDelay>
    </statBases>
    <thingCategories>
      <li>ApparelUtility</li>
    </thingCategories>
    <generateAllowChance>0.15</generateAllowChance>
    <apparel>
	  <wornGraphicPath>Things/Apparel/Backpack/Backpack</wornGraphicPath>
      <careIfWornByCorpse>false</careIfWornByCorpse>
      <careIfDamaged>false</careIfDamaged>
      <bodyPartGroups>
        <li>Torso</li>
      </bodyPartGroups>
      <layers>
        <li>Belt</li>
      </layers>
      <tags>
        <li>VAEA_Utility_Neolithic</li>
		<li>VAEA_Utility_Industrial</li>
